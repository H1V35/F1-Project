<div class='container mt-4'>

  <div class='row justify-content-between'>

    <div class='col-3'>
      <div class='btn-group shadow' role='group' aria-label='Basic radio toggle button group'>
        <input type='radio' class='btn-check' name='btnradio' id='general' autocomplete='off' />
        <a href='/community/GENERAL' class='btn btn-outline-primary' for='general'>GENERAL</a>

        <input type='radio' class='btn-check' name='btnradio' id='team' autocomplete='off' />
        <a href='/community/{{loggedUser.team}}' class='btn btn-outline-primary' for='team'>TEAM</a>
      </div>
    </div>

    <div class='col-1'>
      <div>
        <a href='/community/users' class='btn btn-dark btn-login btn-index shadow'>USERS</a>
      </div>
    </div>
  </div>

  <hr />

  {{#each users}}
    <h2>{{username}}</h2>
    <a href='/community/user/{{username}}'>View</a>
    <hr />
  {{/each}}

</div>